.c-tips {
    text-align: center;
    position: absolute;
    top: 30%;
    left: 30px;
    right: 30px;
    border-radius: 2px;
    z-index: 999;
    display: block;
    background-color: #fff;
    height: 45px;
    line-height: 45px;
    background-color: #666;
    color: #fff;
}

.c-tips-
right {
    color: #55C151;
}

.c-tips-error {
    color: #EC6666;
}

.c-tips-normal {
    color: #3B3B3B;
}

.c-add-user-car {
    position: relative;
    border-top: 1px solid #e7e7e7;
    border-bottom: 1px solid #e7e7e7;
    margin-top: 10px;
    margin-bottom: 100px;
    background-color: #fff;
}

.c-add-user-car-word {
    height: 40px;
    font-size: 12px;
    line-height: 40px;
    text-align: center;
    color: #3e94ff;
}

.c-user-car {
    position: relative;
    border-top: 1px solid #e7e7e7;
    border-bottom: 1px solid #e7e7e7;
    margin-bottom: 10px;
    padding-left: 15px;
    height: 64px;
    background-color: #fff;
}

.c-user-car:last-of-type {
    margin-bottom: 0;
}

.c-user-car-logo {
    margin-top: 12px;
    margin-right: 10px;
    height: 40px;
}

.c-user-car-name {
    color: #333;
}

.c-user-car-code {
    font-size: 12px;
    color: #778187;
}

.android .c-add-icon,
.android .c-mycar-go-personal .igo-personal {
    margin-top: -4px;
}

.c-add-icon,
.c-mycar-go-personal .igo-personal {
    margin-top: -2px;
    margin-right: 8px;
    line-height: 0;
    vertical-align: middle;
}

.c-add-icon-text,
.c-mycar-go-personal-text {
    display: inline-block;
    height: 40px;
    line-height: 40px;
}
.c-add-icon-text{
    font-size: 13px;
}
.c-mycar-go-personal-text {
    font-size: 16px;
}
.c-user-car-title {
    padding-left: 15px;
    height: 25px;
    font-size: 11px;
    line-height: 25px;
    color: #778187;
}

.c-add-user-car-empty {
    display: none;
    text-align: center;
}

.c-add-user-car-empty .iempty-car-logo {
    margin-top: 68px;
}

.c-empty-car-text1 {
    margin-top: 25px;
    height: 16px;
    font-size: 15px;
    line-height: 16px;
    color: #d6deec;
}

.c-add-user-car-empty .c-add-user-car-word {
    border-radius: 5px;
    margin: 37px 15px 8px 15px;
    font-size: 14px;
    background-color: #4497ff;
    color: #fff;
}

.c-empty-car-text2 {
    font-size: 12px;
    color: #97a7c0;
}

.c-user-mycar-list {
    margin-top: 10px;
}

.c-user-car-name-code-box {
    display: table;
    height: 64px;
}

.c-user-car-name-code {
    display: table-cell;
    vertical-align: middle;
}

.c-mycar-go-personal {
    position: absolute;
    z-index: 10;
    bottom: 23px;
    left: 10px;
    right: 10px;
    border-radius: 4px;
    height: 40px;
    line-height: 0;
    text-align: center;
    background-color: #4497ff;
    color: #fff;
}
<div class="c-car-container">
    <ul>
        {#result}
            <li class="c-car-word j-car-word-{key}"><span>{key}</span></li>
            {#cars}
                <li class="c-car-brand j-car-brand {@if cond="{$idx} == 0"}c-first{/if}" data-id="{bid}" data-name="{bname}" data-logo="{blogo}">
                    <img class="c-car-brand-logo" src="{carLogo}{blogo}">
                    <span>{bname}</span>
                </li>
            {/cars}
        {/result}
    </ul>
</div>
<div class="c-addcar-box">
    <div class="c-addcar-info-car j-addcar-select-car flex-container" data-cid="{cid}" data-ucid="{ucid}">
        <label class="c-addcar-label">车辆型号</label>
        <div class="c-addcar-car-name flex-auto1">
            {cname}
        </div>
        <div class="c-addcar-car-name-box">
            <i class="iright"></i>
        </div>
    </div>
    <div class="c-addcar-info flex-container">
        <label class="c-addcar-label">车牌号码</label>
        <span class="c-addcar-uccode1 j-addcar-uccode1 flex-auto1">
            <span class="j-addcar-uccode1-text">{uccode1}</span>
            <i class="idown"></i>
        </span>
        <input class="c-addcar-uccode2 flex-auto3" value="{uccode2}" placeholder="请输入车牌号">
    </div>
    <div class="c-addcar-info flex-container">
        <label class="c-addcar-label">购买日期</label>
        <div class="c-date-placeholder {?ucdate}hide{/ucdate}">请选择购买日期</div>
        <input type="date" class="j-addcar-date c-addcar-date flex-auto1" value="{ucdate}">
        <i class="iright"></i>
    </div>
</div>
{?ucid}
<div class="c-addcar-submit j-addcar-submit">保存</div>
<div class="c-addcar-delete j-addcar-delete">删除车辆</div>
{:else}
<div class="c-addcar-submit j-addcar-submit">确认添加</div>
{/ucid}
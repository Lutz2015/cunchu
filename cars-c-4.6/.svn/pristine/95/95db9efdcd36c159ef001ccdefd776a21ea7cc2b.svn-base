
@font-face {font-family: "iconfont";
  src: url('/common/svg/iconfont.eot'); /* IE9*/
  src: url('/common/svg/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
      url('/common/svg/iconfont.woff') format('woff'), /* chrome、firefox */
      url('/common/svg/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
      url('/common/svg/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;/*平滑  抗锯齿*/
  -webkit-text-stroke-width: 0.2px;/*用长度值指定描边厚度。不允许负值*/
  -moz-osx-font-smoothing: grayscale;
}

.c-header-name{  
    height:87px;
    background:#fff;
    position:relative; 
    line-height:87px; 
    border-bottom: 1px solid #e7e7e7;
    margin-bottom: 10px;
}
.c-h-bg{ 
    float:left; 
    width:62px; 
    height:62px; 
    background:url(http://developer.baidu.com/static/btn_bg.png) no-repeat; 
    background-size:100%; 
    position:absolute; 
    left:12px; 
    top:12px; 
    z-index:2;
}
.c-h-name{ 
    padding-left:15px; 
    float:left;
    font-size: 18px;
    color: #333;
    font-family: Arial;
}
.c-h-img{ 
    float:left; 
    margin-top:12px; 
    width:62px; 
    height:62px; 
    overflow:hidden; 
    margin-left:12px;
    position: relative;
}
.c-h-img img{
    vertical-align: top;
    width: auto;
    height: auto;
    position: absolute;
    left:50%;
    top:50%;
    margin-left: -31px;
    margin-top: -31px;
}
.c-personal-c p{ 
    line-height:45px; 
    height:45px; 
    background:url(http://developer.baidu.com/static/bgg.png) no-repeat; 
    padding-left:50px; 
    font-size:14px; 
    background-color:#fff; 
    color:#333; 
    background-size: 18px;
}

p.c-p-order{
    background-position: 13px -28px;
    height: 44px;
    border-bottom: 1px solid #e7e7e7;
}

p.c-my-car{
    background-position: 13px -130px;
}
p.c-my-list,
p.c-my-car,
p.c-my-one{
    border-top: 1px solid #e7e7e7;
    border-bottom: 1px solid #e7e7e7;
}
p.c-my-list{
    background-position: 13px -78px;
    border-top: 0;
}
p.c-my-one{
    background-position: 13px 17px;
    border-bottom: 0;
    height: 46px;
}
.c-personal-c a{
    text-decoration:none;
    display: block;
    overflow: hidden;
    margin-bottom: 10px;
}
.c-personal-c a:first-child{
    margin-bottom: 0;
}
.c-personal-c hr{
    margin: -1px 0 0 13px;
    border: 0;
    border-bottom:1px solid #e7e7e7;
}
.c-personal-c .iright{
    float:right;
    margin-right:13px;
    margin-top:17px;
}

.c-personal-go-nav {
    position: absolute;
    z-index: 10;
    bottom: 23px;
    left: 10px;
    right: 10px;
    border-radius: 4px;
    height: 40px;
    font-size: 13px;
    line-height: 0;
    text-align: center;
    background-color: #4497ff;
    color: #fff;
}

.c-personal-go-nav .igo-nav {
    margin-top: -2px;
    margin-right: 8px;
    line-height: 0;
    vertical-align: middle;
}

.c-personal-go-nav-text {
    display: inline-block;
    height: 40px;
    font-size: 16px;
    line-height: 40px;
}
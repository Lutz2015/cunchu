.c-img-slider {
    position: relative;
    top: 0;
    width: 100%;
    height: 240px;
    overflow: hidden;
}

/*@media screen and (max-height: 480px) {
    .c-img-slider {
        height: 140px;
    }
}*/

.c-img-slider-list {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 240px;
}

.c-img-slider-item {
    position: absolute;
    display: block;
    width: 100%;
    height: 240px;
}

.c-img-slider-item img {
    width: 100%;
    height: 100%;
}

.c-nav-box {
    margin-top: 10px;
    background-color: #fff;
    margin-bottom: 10px;
}

.c-nav-item {

}
.c-left-maintain ,.c-right-newcar{
    border-radius: 2px;
    width: 145px;
    height: 160px;
    background-color: #f4f6fa;
}

.c-nav-item .inav-tool {
    margin-top: 20px;
    margin-bottom: 10px;
    margin-left: 32px;
}

.c-nav-item .inav-insurance {
    margin-top: 22px;
    margin-bottom: 13px;
    margin-left: 32px;
}

.c-nav-item .iquestion-mark {
    margin-top: 20px;
    margin-bottom: 14px;
    margin-left: 42px;
}

.c-nav-item-text {
    display: block;
    text-align: center;
    font-size: 16px;
    color: #333;
    margin-top: 10px;
}

.c-nav-goto-home {
    position: absolute;
    right: 5px;
    bottom: 80px;
    left: 5px;
    border-radius: 2px;
    height: 40px;
    line-height: 40px;
    background-color: #fff;
}

.c-nav-goto-home-text {
    display: block;
    text-align: center;
    color: #218ff3;
}

.c-nav-goto-home .iright {
    position: absolute;
    top: 16px;
    right: 15px;
}

.c-nav-tel {
    position: relative;
    display: block;
    border-radius: 12px;
    margin: 0 auto;
    padding-left: 10px;
    width: 160px;
    text-align: center;
    font-size: 16px;
    color: #778089;
    font-family: "Microsoft YaHei";
}

.c-nav-tel-box {
    width: 90%;
    height: 40px;
    line-height: 40px;
    border-radius: 5px;
    border: 1px solid #29a0f4;
    margin: 0 auto;
    margin-bottom: 45px;
}

.c-nav-tel-box .itel {
    position: absolute;
    top: 6px;
    left: 15px;
}
.circle-icon{
    display: block;
    margin: 21px auto 0 auto;
    width: 90px;
    height: 90px;
    /*background-color: #c5e5fb;*/
    -moz-border-radius: 50px;
    -webkit-border-radius: 50px;
    border-radius: 50px;
}
.personalCenter{
    width: 73px;
    /*padding-right: 15px;*/
    height: 24px;
    line-height: 24px;
    border-radius: 3px;
    text-align: center;
    background-color: #fff;
    border: 1px solid #ccc;
    position: absolute;
    right: 10px;
    top: 10px;
    opacity: 0.8;
    display: table;
}
.personalCenterPar{
    vertical-align: middle;
    display: table-cell;
}
.c-center-icon{
    height: 28px;
    width: 28px;
    background-image: url('http://developer.baidu.com/static/peoplecenter.png') ;
    background-repeat: no-repeat;
    background-size: 54%;
    position: absolute;
    top: 4px;
    left: 6px;
}
.c-center-icon-login{
    height: 12px;
    width: 11px;
 /*   background-image: url('http://developer.baidu.com/static/loginpeolple.png') ;
    background-repeat: no-repeat;
    background-size: 100%;
    position: absolute;
    top: 6px;
    left: 10px;*/
}
.c-center-text{
    font-size: 12px;
    /*position: absolute;
    top: 7px;
    right: 10px;*/
}
.indicator{
    position: absolute;
    right: 16px;
    bottom: 6px;
}
.indicator span{
    width: 4px;
    height: 4px;
    display: inline-block;
    background-color: #cccccc;
    margin-right: 5px;
    border-radius: 6px;
}
.cur{
    background-color: #ffffff !important;
}
#navcontent{
    background-color: #fff;
}
.c-img-slider-item a{
    display: block;
    width: 100%;
    height: 100%;

}
.c-nav-line{
    width: 300px;
    margin: 0 auto;
    margin-bottom: 10px;
    border-bottom: 1px solid #e7e7e7;
}
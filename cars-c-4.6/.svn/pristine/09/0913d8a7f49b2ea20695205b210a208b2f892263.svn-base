.c-dialog {
    position: fixed;
    z-index: 800;
    top: 15%;
    width: 90%;
    left: 45%;
    margin-left: -40%;
}

.c-dialog.thirty {
    top: 30%;
}

.c-dialog-header {
    border-bottom: 1px solid #e7e7e7;
    height: 40px;
    font-size: 16px;
    line-height: 40px;
    text-align: center;
    color: #262626;
    background-color: #fff;
}

.c-dialog-content {
    padding-top: 12px;
    padding-right: 16px;
    padding-left: 16px;
    max-height: 200px;
    overflow-y: auto;
    background-color: #fff;
}

.c-dialog-footer {
    border-top: 1px solid #e7e7e7;
    height: 40px;
    font-size: 16px;
    line-height: 40px;
    text-align: center;
    background-color: #fafafa;
    color: #373737;
}

.c-dialog-hascancel .c-dialog-cancel {
    display: block;
    float: left;
    width: 49%;
    border-right: 1px solid #e7e7e7;
}

.c-dialog-confirm {
    display: block;
}

.c-dialog-hascancel .c-dialog-confirm {
    float: right;
    width: 50%;
    color: #4497ff;
}
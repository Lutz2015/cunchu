.c-dialog {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 100%;
    /*background: #0086b3;*/
    background: rgba(255,255,255 ,0)
}
.c-dialog-bg {
    /*position: fixed;*/
    /*top: 15%;*/
    /*z-index: 999;*/
    position: absolute;
    top: 15%;
    left: 15%;
    z-index: 10000;
    margin-left: 220px;
    border-radius: 5px;
    box-shadow: 0 5px 15px rgba(0, 0, 0, .5);
    background-color: #fff;
}

.c-dialog-header {
    text-align: left;
    padding: 5px 15px;
    border-bottom: 1px solid #e0e0e0;
    overflow: hidden;
}

.c-dialog-footer {
    overflow: hidden;
    padding: 15px;
    border-top: 1px solid #e7e7e7;
}

.c-dialog-close,
.c-dialog-default,
.c-dialog-confirm {
    display: inline-block;
    padding: 6px 12px;
    margin-bottom: 0;
    font-size: 14px;
    font-weight: 400;
    text-align: center;
    border-radius: 5px;
    cursor: pointer;
}

.c-dialog-close {
    float: right;
    font-size: 21px;
    font-weight: 700;
    color: #000;
    text-shadow: 0 1px 0 #fff;
    opacity: .2;
    margin-top: 10px;
}

.c-dialog-confirm {
    margin-left: 20px;
    border-color: #e0e0e0;
}

.c-dialog-close:hover,
.c-dialog-default:hover,
.c-dialog-confirm:hover {
    background-color: #337ab7;
    color: #fff;
}

.c-dialog-sure {
    width: 400px;
    height: 50px;
    line-height: 50px;
    font-size: 16px;
    text-align: center;
    color: #337ab7;
}
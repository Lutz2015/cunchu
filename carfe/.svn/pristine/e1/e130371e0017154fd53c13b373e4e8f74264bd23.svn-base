.c-aside {
    padding-left: 10px;
    border-right: 1px solid #333;
    width: 149px;
    height: 100%;
    background-color: #1D2124;
    color: #fff;
}

.c-aside-type {
    display: block;
    font-size: 16px;
    height: 40px;
    line-height: 40px;
    color: #fff;
    text-decoration: none;
    padding-left: 10px;
}

.c-aside-type:hover {
    background: #4077e9;
}

.c-aside-list {
    margin-top: 5px;
    cursor: pointer;
}


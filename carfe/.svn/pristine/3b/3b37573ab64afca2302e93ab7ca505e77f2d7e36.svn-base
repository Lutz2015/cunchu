.c-care-content {
    overflow-y: auto;
    margin-top: 30px;
    margin-right: 60px;
    margin-left: 60px;
}

.c-care-tab {
    border: solid #ccc;
    border-width: 1px 0 0 1px;
    margin-top: 20px;
    margin-bottom: 30px;
    width: 100%;
    height: 800px;
    text-align: center;
}

.c-care-thead {
    height: 35px;
    line-height: 35px;
    background-color: #474F62;
    vertical-align: middle;
    border-color: inherit;
}

.c-care-span {
    height: 30px;
    line-height: 30px;
    padding: 0 40px;
}

.c-care-create {
    width: 60px;
    height: 35px;
    background-color: #2E8FD4;
    color: #fff;
    cursor: pointer;
}

.c-newcare-content {
    padding: 15px 20px;
    width: 600px;
}
.c-newcare-item {
    height: 30px;
    line-height: 30px;
    margin-top: 10px;
}
.c-newcare-lab {
    display: inline-block;
    width: 80px;
    text-align: left;
}

.c-newcare {
    display: inline-block;
    width: 200px;
    height: 30px;
    line-height: 30px;
    padding-left: 5px;
    text-align: left;
}

.c-newcare-check {
    display: inline-block;
    margin-left: 10px;
    width: 200px;
    color: #EE5757;
}

.c-choose-content {
    height: 100px;
    border-bottom: 1px solid #dfdfdf;
}


.c-choose-form {
    margin-top: 20px;
    padding: 10px 20px;
}

.c-choose-label {
    position: relative;
}

.c-label {
    margin-right: 10px;
}

.c-choose-brand, .c-choose-series {
    position: relative;
    width: 136px;
    height: 30px;
    line-height: 30px;
    margin-right: 20px;
    margin-top: 4px;
    border: 1px solid #e0e0e0;
    padding-left: 6px;
}

.c-choose-search {
    width: 74px;
    height: 37px;
    line-height: 35px;
    font-size: 18px;
    text-align: center;
    background: #4077e9;
    color: #fff;
}

.c-label-iconfont {
    position: absolute;
    right: 26px;
    top: 14px;
    width: 15px;
    height: 14px;
    background: url("http://vouchers.baidu.com/static/vouchers/console_res//market/img/coupon-icon-type.png") no-repeat 0 0;
}
.c-choose-down {
    position: absolute;
    top: 36px;
    left: 42px;
    z-index: 100;
}
.c-brand-down {
    left: 41px;
}
.c-choose-tab {
    width: 380px;
    height: 260px;
    background: #fff;
    border: 1px solid #4497ff;
}

.c-tab-hotseries,
.c-tab-hotbrand {
    height: 30px;
    line-height: 28px;
    background: #e7f2ff;;
}

.c-tab-item {
    font-size: 12px;
    cursor: pointer;
    display: inline-block;
    padding: 0 10px;
}

.hotbrand-select,
.series-select {
    color: #4497ff;
    border-bottom: #4497ff solid 2px;
}

.c-tab-carbrand ,
.c-tab-carseries {
    height: 230px;
    display: none;
    overflow-y: auto;
}

.car-brand,
.car-series {
    display: inline-block;
    padding: 12px 10px;
    cursor: pointer;

}
.car-brand:hover,
.car-series:hover {
    background: #4497ff;
    color: #fff;
}
.addcar-color {
    background: #eee;
}

.carbrand-select,
.carseries-select {
    display: block;
}

.hover {
    background: #E0F2BE;
}

